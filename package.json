{
  "name": "@olton/cake",
  "title": "Cake",
  "description": "Cake is a minimalist JavaScript library that parses, validates, manipulates, and displays strings for modern browsers with comfortable modern API.",
  "version": "1.0.0",

  "main": "./dist/index.js",
  "browser": "./lib/index.js",
  "module": "./module/index.js",

  "homepage": "https://github.com/olton/cake",
  "author": {
    "name": "Serhii Pimenov",
    "url": "https://pimenov.com.ua"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olton/cake.git"
  },
  "keywords": [
    "string",
    "javascript",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/olton/cake/issues"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build-lib": "cross-env BABEL_ENV=browser babel ./src --out-dir ./lib --copy-files",
    "build": "npm run build-lib"
  },
  "jest": {
    "verbose": false,
    "bail": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.js?$",
    "collectCoverage": true
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3"
  },
  "dependencies": {}
}
